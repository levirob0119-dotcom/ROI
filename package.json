{
  "name": "fx-page-example",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,ts,json,css,md,vue}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "parser": "markdown-nocjsp"
        }
      }
    ]
  },
  "dependencies": {
    "@nio-fe/nio-def-core": "^1.0.36",
    "axios": "^1.4.0",
    "vite-plugin-mkcert": "^1.15.0",
    "vue": "^3.2.47",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.8",
    "prettier-plugin-md-nocjsp": "^1.1.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vue-tsc": "^1.6.1"
  }
}
